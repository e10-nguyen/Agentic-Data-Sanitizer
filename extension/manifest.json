{
  "manifest_version": 3,
  "name": "Agentic Data Sanitizer",
  "version": "1.0",
  "description": "Chrome extension for data sanitization",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
